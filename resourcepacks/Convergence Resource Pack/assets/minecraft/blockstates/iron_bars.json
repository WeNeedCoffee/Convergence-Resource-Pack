{
    "multipart": [
        {   "apply": { "model": "iron_bars_post_ends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": [
			{ "model": "iron_bars_post" },
			{ "model": "iron_bars_post_0" },
			{ "model": "iron_bars_post_1" },
			{ "model": "iron_bars_post_2" },
			{ "model": "iron_bars_post_3" },
			{ "model": "iron_bars_post_4" }
        ]},
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": [
			{ "model": "iron_bars_cap" },
			{ "model": "iron_bars_cap_0" },
			{ "model": "iron_bars_cap_1" },
			{ "model": "iron_bars_cap_2" },
			{ "model": "iron_bars_cap_3" },
			{ "model": "iron_bars_cap_4" }
        ]},
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": [
			{ "model": "iron_bars_cap", "y": 90 },
			{ "model": "iron_bars_cap_0", "y": 90 },
			{ "model": "iron_bars_cap_1", "y": 90 },
			{ "model": "iron_bars_cap_2", "y": 90 },
			{ "model": "iron_bars_cap_3", "y": 90 },
			{ "model": "iron_bars_cap_4", "y": 90 }
        ]},
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": [
			{ "model": "iron_bars_cap_alt" },
			{ "model": "iron_bars_cap_alt_0" },
			{ "model": "iron_bars_cap_alt_1" },
			{ "model": "iron_bars_cap_alt_2" },
			{ "model": "iron_bars_cap_alt_3" },
			{ "model": "iron_bars_cap_alt_4" }
        ]},
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": [
			{ "model": "iron_bars_cap_alt", "y": 90 },
			{ "model": "iron_bars_cap_alt_0", "y": 90 },
			{ "model": "iron_bars_cap_alt_1", "y": 90 },
			{ "model": "iron_bars_cap_alt_2", "y": 90 },
			{ "model": "iron_bars_cap_alt_3", "y": 90 },
			{ "model": "iron_bars_cap_alt_4", "y": 90 }
        ]},
        {   "when": { "north": true },
            "apply": [
			{ "model": "iron_bars_side" },
			{ "model": "iron_bars_side_0" },
			{ "model": "iron_bars_side_1" },
			{ "model": "iron_bars_side_2" },
			{ "model": "iron_bars_side_3" },
			{ "model": "iron_bars_side_4" }
        ]},
        {   "when": { "east": true },
            "apply": [
			{ "model": "iron_bars_side", "y": 90 },
			{ "model": "iron_bars_side_0", "y": 90 },
			{ "model": "iron_bars_side_1", "y": 90 },
			{ "model": "iron_bars_side_2", "y": 90 },
			{ "model": "iron_bars_side_3", "y": 90 },
			{ "model": "iron_bars_side_4", "y": 90 }
        ]},
        {   "when": { "south": true },
            "apply": [
			{ "model": "iron_bars_side_alt" },
			{ "model": "iron_bars_side_alt_0" },
			{ "model": "iron_bars_side_alt_1" },
			{ "model": "iron_bars_side_alt_2" },
			{ "model": "iron_bars_side_alt_3" },
			{ "model": "iron_bars_side_alt_4" }
        ]},
        {   "when": { "west": true },
            "apply": [
			{ "model": "iron_bars_side_alt", "y": 90 },
			{ "model": "iron_bars_side_alt_0", "y": 90 },
			{ "model": "iron_bars_side_alt_1", "y": 90 },
			{ "model": "iron_bars_side_alt_2", "y": 90 },
			{ "model": "iron_bars_side_alt_3", "y": 90 },
			{ "model": "iron_bars_side_alt_4", "y": 90 }
        ]}
    ]
}
